<apex:page controller="Wizard_Class">
    <apex:form >
        <apex:pageBlock title="Contact Details">
            <apex:pageblockButtons >
                <apex:commandButton value="Previous Page" action="{!goto_accountPage}"/>
                <apex:commandButton value="Next Page" action="{!goto_opportunityPage}"/>
                <!--<apex:commandButton value="Review" action="{!goto_reviewPage}"/>-->
            </apex:pageblockButtons>
                
            <apex:pageblockSection columns="1">
                <apex:inputField value="{!c.FirstName}"/>
                <apex:inputField value="{!c.LastName}"/>
                <apex:inputField value="{!c.Email}"/>
                <apex:inputField value="{!c.Phone}"/>
                <apex:inputField value="{!c.Fax}"/>
            </apex:pageblockSection>                            
            
        </apex:pageBlock>
    </apex:form>
</apex:page>